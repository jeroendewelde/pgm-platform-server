{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "periode": {
      "type": "integer",
      "default": 1,
      "unique": false,
      "required": false,
      "max": 8
    },
    "learning_line": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::learning-line.learning-line",
      "inversedBy": "courses"
    },
    "specialisations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::specialisation.specialisation",
      "inversedBy": "courses"
    },
    "academic_year": {
      "type": "string",
      "regex": "^\\d{4}-\\d{4}$"
    },
    "Tag": {
      "type": "component",
      "repeatable": true,
      "component": "tag.tag"
    },
    "teachers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::teacher.teacher",
      "inversedBy": "courses"
    },
    "Description": {
      "type": "richtext"
    },
    "images": {
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "type": "media",
      "multiple": true
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "course"
    }
  }
}
